<!doctype html>
<html class="no-js">
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta name="description" content="">
		<meta name="author" content="">

		<title>DataPlay</title>

		<link rel="shortcut icon" href="images/favicon.png">

		<!-- build:css(.) styles/vendor.css -->
		<!-- bower:css -->
		<link rel="stylesheet" href="bower_components/bootswatch/darkly/bootstrap.css" />
		<link rel="stylesheet" href="bower_components/nvd3/nv.d3.css" />
		<!-- endbower -->
		<!-- endbuild -->

		<!-- build:css(.tmp) styles/main.css -->
		<link rel="stylesheet" href="styles/layout.css">
		<link rel="stylesheet" href="styles/charts.css">
		<link rel="stylesheet" href="styles/maptest.css">
		<link rel="stylesheet" href="styles/signin.css">
		<!-- endbuild -->
	</head>

	<body ng-app="dataplayApp">
		<!--[if lt IE 8]>
			<p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
		<![endif]-->

		<!-- Fixed navbar -->
		<div class="navbar navbar-default navbar-fixed-top" role="navigation" ng-controller="HomeCtrl">
			<div class="container">
				<div class="navbar-header">
					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<a class="navbar-brand" href="/">DataPlay</a>
				</div>
				<div class="navbar-collapse collapse">
					<ul class="nav navbar-nav">
						<li ng-class="{active: isActive('/')}" ng-show="Auth.isAuthenticated() !== false"><a href="/">Home</a></li>
					</ul>
					<ul class="nav navbar-nav navbar-right">
						<li ng-class="{active: isActive('/login')}" ng-hide="Auth.isAuthenticated() !== false"><a ng-href="/login">Login</a></li>
						<li ng-class="{active: isActive('/register')}" ng-hide="Auth.isAuthenticated() !== false"><a ng-href="/register">Register</a></li>
						<li ng-show="Auth.isAuthenticated() !== false"><a href="#" ng-controller="UserCtrl" ng-click="logout()">Logout</a></li>
					</ul>
				</div><!--/.nav-collapse -->
			</div>
		</div>

		<!-- Add your site or application content here -->
		<div class="container">
			<div class="row">
				<div class="col-md-12" ng-view=""></div>
			</div>
		</div>

		<div class="footer">
			<div class="container">
				<p class="text-muted">Powered by <strong><em>PlayGen</em></strong> &copy; 2014.</p>
			</div>
		</div>

		<!-- Google Analytics -->
		<!-- <script>
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

			ga('create', 'UA-XXXX-Y', 'auto');
			ga('send', 'pageview');
		</script> -->
		<!-- End Google Analytics -->

		<!-- build:js(.) scripts/ie.js -->
		<!-- bower:js -->
		<!--[if lt IE 9]>
		<script src="bower_components/html5shiv/dist/html5shiv.js"></script>
		<script src="bower_components/respond/dest/respond.src.js"></script>
		<script src="bower_components/es5-shim/es5-shim.js"></script>
		<script src="bower_components/json3/lib/json3.js"></script>
		<![endif]-->
		<!-- endbower -->
		<!-- endbuild -->

		<!-- build:js(.) scripts/vendor.js -->
		<!-- bower:js -->
		<script src="bower_components/d3/d3.js"></script>
		<script src="bower_components/crossfilter/crossfilter.js"></script>
		<script src="bower_components/nvd3/nv.d3.js"></script>

		<script src="bower_components/angular/angular.js"></script>
		<script src="bower_components/angular-resource/angular-resource.js"></script>
		<script src="bower_components/angular-cookies/angular-cookies.js"></script>
		<script src="bower_components/angular-sanitize/angular-sanitize.js"></script>
		<script src="bower_components/angular-animate/angular-animate.js"></script>
		<script src="bower_components/angular-route/angular-route.js"></script>
		<script src="bower_components/angular-cookie/angular-cookie.js"></script>
		<script src="bower_components/angular-bootstrap/ui-bootstrap.js"></script>
		<script src="bower_components/angular-bootstrap/ui-bootstrap-tpls.js"></script>
		<script src="bower_components/angular-nvd3/dist/angular-nvd3.js"></script>
		<!-- endbower -->
		<!-- endbuild -->

		<!-- build:js({.tmp,app}) scripts/app.js -->
		<script src="scripts/app.js"></script>

		<script src="scripts/directives/match.js"></script>

		<script src="scripts/services/auth.js"></script>
		<script src="scripts/services/user.js"></script>
		<script src="scripts/services/overview.js"></script>
		<script src="scripts/services/requestinterceptor.js"></script>
		<script src="scripts/services/patternmatcher.js"></script>

		<script src="scripts/routes.js"></script>

		<script src="scripts/controllers/home.js"></script>
		<script src="scripts/controllers/search.js"></script>
		<script src="scripts/controllers/overview.js"></script>
		<script src="scripts/controllers/charts.js"></script>
		<script src="scripts/controllers/user.js"></script>
		<!-- endbuild -->
	</body>
</html>
